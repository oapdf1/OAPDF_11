function checkMainSearchField(event){
    var searchTextBox = $("#searchText");
    if(searchTextBox[0].value == null || searchTextBox[0].value.trim() == ""){
        alert("Please enter search terms before running your search.");
        return false;
    }else if(searchTextBox[0].value.indexOf("10.1021/") != -1){
        event.preventDefault();
        redirectToPublicationPage(searchTextBox);
    }

    var mainSearch = $('.mainSearchButton');
    if(mainSearch != "undefined" && mainSearch.length > 0){
        mainSearch[0].click();
    }

}
function redirectToPublicationPage() {
    var doi=$('#searchText')[0].value;

    // detect manuscript ids and prepend them with 10.1021/ to turn them into valid DOIs
    if (doi.indexOf('10.1021/') == -1) {
        doi="10.1021/"+doi;
    }
    else if (doi.length < 10) {
        alert('Please enter a valid ACS Publications DOI before running your search.');
        return false;
    }

    // detect book DOIs
    if (doi.indexOf('10.1021/bk-') != -1 || doi.indexOf('10.1021/ba-') != -1) {
        // detect books
        if (doi.length < 21) {
            parent.window.location="/doi/book/" + encodeURIComponent(doi);return false;
        }
        // all others must be chapters
        else  parent.window.location="/doi/abs/" + encodeURIComponent(doi); return false
    }
    else parent.window.location="/doi/abs/" + encodeURIComponent(doi); return false;
}
